# Size
localhost:8080/sizes/add?name=Default&sizes=Small,Medium,Large

# Modifiers
localhost:8080/sizes/add?name=Brew Coffee Modifier&description=Modifiers for nitro brew coffee&type=coffee

# Categories
localhost:8080/categories/add?name=Coffee

# Consumable
localhost:8080/consumables/add?name=Nitro%20Coffee&description=This%20is%20Nitro%20Brew%0Coffee&price=150&category_id=1&modifier_id=1&size_id=1


